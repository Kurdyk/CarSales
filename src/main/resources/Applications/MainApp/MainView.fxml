<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Applications.MainApp.MainAppController">
   <children>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <ImageView fx:id="cars" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onCarsClick" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Cars.jpeg" />
               </image>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </ImageView>
            <ImageView fx:id="clients" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onClientsClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@ClientFiles.jpeg" />
               </image>
            </ImageView>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <ImageView fx:id="newOrder" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onNewOrderClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@Order.jpeg" />
               </image>
            </ImageView>
            <ImageView fx:id="oldOrders" fitHeight="150.0" fitWidth="200.0" onMouseClicked="#onOldOrderClicked" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@PreviousOrders.png" />
               </image>
            </ImageView>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
